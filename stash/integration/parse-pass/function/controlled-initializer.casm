function (controlled) a: -> Integer initially { 1 }
function (controlled) b: Integer -> Integer initially { 1 -> 2, 2 -> 3, 3 -> 4 }
function c: -> Integer initially { 1 }
function d: Integer -> Integer initially { 1 -> 2, 2 -> 3, 3 -> 4 }

init main

rule main = {
  assert a = 1
  assert b( 1 ) = 2
  assert b( 2 ) = 3
  assert b( 3 ) = 4

  assert c = 1
  assert d( 1 ) = 2
  assert d( 2 ) = 3
  assert d( 3 ) = 4

  program( self ) := undef
}
