// cmdline "-d trace"
CASM fora

enum en = { e1, e2, e3, e4, e5 }

rule main = {|
  forall i in [1..3] do {|
    debug trace "i = " + i
  |}

  forall x in [1,4,7] do {|
    debug trace "x = " + x
  |}

  forall x in 5 do {|
    debug trace "x = " + x
  |}

  forall x in -5 do {|
    debug trace "x = " + x
  |}

  forall x in en do {
    debug trace "x = " + x
  }

  program(self) := undef
|}

init main

