CASM forall2

init initR

enum E = { one, two, three }
function pipe : E -> List(String) initially { one -> ["one"], two -> ["two"], three -> ["three"] }

rule initR= {
	forall s in E do
	let e_rev:E = Integer2Enum(Enum2Integer(three) - Enum2Integer(s)) in
	forall o in pipe(e_rev) do
	{|
		print o
	|}
	program(self):=undef
}
