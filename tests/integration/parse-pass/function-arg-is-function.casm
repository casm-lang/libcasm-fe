function i: -> Integer initially {1}
function bar: Integer -> Integer initially {0 -> 0}
function foo: Integer -> Integer initially {1 -> 1}

init main

rule main = {
    i := i+1
    foo(i+1) := i+1
    bar(foo(i)) := i
    assert bar(i-1) = i-1

    if i = 100 then {
        print i
        program(self) := undef
    }
}
